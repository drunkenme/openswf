# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  01_unit_test_config = debug
  02_simple_shape_config = debug
endif

PROJECTS := 01-unit-test 02-simple-shape

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

01-unit-test:
ifneq (,$(01_unit_test_config))
	@echo "==== Building 01-unit-test ($(01_unit_test_config)) ===="
	@${MAKE} --no-print-directory -C . -f 01-unit-test.make config=$(01_unit_test_config)
endif

02-simple-shape:
ifneq (,$(02_simple_shape_config))
	@echo "==== Building 02-simple-shape ($(02_simple_shape_config)) ===="
	@${MAKE} --no-print-directory -C . -f 02-simple-shape.make config=$(02_simple_shape_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f 01-unit-test.make clean
	@${MAKE} --no-print-directory -C . -f 02-simple-shape.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   01-unit-test"
	@echo "   02-simple-shape"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"