# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  openswf_config = debug
endif
ifeq ($(config),release)
  openswf_config = release
endif

PROJECTS := openswf

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

openswf:
ifneq (,$(openswf_config))
	@echo "==== Building openswf ($(openswf_config)) ===="
	@${MAKE} --no-print-directory -C . -f openswf.make config=$(openswf_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f openswf.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   openswf"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"