# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  unit_test_config = debug
endif
ifeq ($(config),release)
  unit_test_config = release
endif

PROJECTS := unit-test

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

unit-test:
ifneq (,$(unit_test_config))
	@echo "==== Building unit-test ($(unit_test_config)) ===="
	@${MAKE} --no-print-directory -C unit-test -f Makefile config=$(unit_test_config)
endif

clean:
	@${MAKE} --no-print-directory -C unit-test -f Makefile clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   unit-test"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"